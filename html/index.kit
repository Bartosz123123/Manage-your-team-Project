<body data-mode="light">

    <!--@include 'nav.kit'-->

    <main class="main">

        <section class="mobile-banner-section">
            <div class="banner-box">
                <div class="button-box">
                    <h2 class="banner-title">Trello brings all your tasks, teammates, and tools together</h2>
                    <p class="banner-text">Keep everything in the same place-even if your team isn’t.</p>

                    <a href="#" class="btn-link">Sign up - it’s free!</a>

                </div>
                <div class="cards-img-box"></div>
            </div>

            <div class="circle"></div>
        </section>

        <div class="clock">
            <div class="clock-box">
                <span class="time hours"></span>
                <span>:</span>
                <span class="time minutes"></span>
                <span>:</span>
                <span class="time seconds"></span>

            </div>
            <div class="day-box">
                <span class="day"></span>
                <span>.</span>
                <span class="mounth"></span>
                <span>.</span>
                <span class="year"></span>
            </div>


            <div class="hero"></div>
        </div>
